<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple_700"
    tools:context=".MainActivity">



    <ImageView
        android:id="@+id/tema"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tema" />

    <TextView
        android:id="@+id/mtk2"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:fontFamily="@font/lemonadabold"
        android:text="ANGKA"
        android:textAlignment="center"
        android:textColor="@color/purple_700"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kembali1" />

    <Button
        android:id="@+id/kembali1"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/ic_baseline_arrow_back_24"
        android:theme="@style/arrow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="264dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.979"
        app:layout_constraintStart_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mtk2">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <Button
                android:id="@+id/satu"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:background="@drawable/button"
                android:backgroundTint="@color/purple_200"
                android:fontFamily="@font/lemonadabold"
                android:text="1"
                android:textColor="@color/purple_700"
                android:textSize="25sp"
                android:theme="@style/button2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/nol"
                app:layout_constraintTop_toTopOf="@+id/nol"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/sembilan"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:background="@drawable/button"
                android:backgroundTint="@color/purple_200"
                android:fontFamily="@font/lemonadabold"
                android:text="9"
                android:textColor="@color/purple_700"
                android:textSize="25sp"
                android:theme="@style/button2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/delapan"
                app:layout_constraintTop_toTopOf="@+id/delapan"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/delapan"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_marginTop="25dp"
                android:background="@drawable/button"
                android:backgroundTint="@color/purple_200"
                android:fontFamily="@font/lemonadabold"
                android:text="8"
                android:textColor="@color/purple_700"
                android:textSize="25sp"
                android:theme="@style/button2"
                app:layout_constraintEnd_toStartOf="@+id/sembilan"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/enam"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/enam"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_marginTop="25dp"
                android:background="@drawable/button"
                android:backgroundTint="@color/purple_200"
                android:fontFamily="@font/lemonadabold"
                android:text="6"
                android:textColor="@color/purple_700"
                android:textSize="25sp"
                android:theme="@style/button2"
                app:layout_constraintEnd_toStartOf="@+id/tujuh"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/empat"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/tujuh"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:background="@drawable/button"
                android:backgroundTint="@color/purple_200"
                android:fontFamily="@font/lemonadabold"
                android:text="7"
                android:textColor="@color/purple_700"
                android:textSize="25sp"
                android:theme="@style/button2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/enam"
                app:layout_constraintTop_toTopOf="@+id/enam"
                tools:ignore="MissingConstraints" />


            <Button
                android:id="@+id/empat"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_marginTop="25dp"
                android:background="@drawable/button"
                android:backgroundTint="@color/purple_200"
                android:fontFamily="@font/lemonadabold"
                android:text="4"
                android:textColor="@color/purple_700"
                android:textSize="25sp"
                android:theme="@style/button2"
                app:layout_constraintEnd_toStartOf="@+id/lima"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dua"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/lima"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:background="@drawable/button"
                android:backgroundTint="@color/purple_200"
                android:fontFamily="@font/lemonadabold"
                android:text="5"
                android:textColor="@color/purple_700"
                android:textSize="25sp"
                android:theme="@style/button2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/empat"
                app:layout_constraintTop_toTopOf="@+id/empat"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/dua"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_marginTop="25dp"
                android:background="@drawable/button"
                android:fontFamily="@font/lemonadabold"
                android:text="2"
                android:textColor="@color/purple_700"
                android:textSize="25sp"
                android:theme="@style/button2"
                app:layout_constraintEnd_toStartOf="@+id/tiga"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/nol"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/nol"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_marginTop="52dp"
                android:background="@drawable/button"
                android:fontFamily="@font/lemonadabold"
                android:text="0"
                android:textColor="@color/purple_700"
                android:textSize="25sp"
                android:theme="@style/button2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/satu"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/tiga"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:background="@drawable/button"
                android:backgroundTint="@color/purple_200"
                android:fontFamily="@font/lemonadabold"
                android:text="3"
                android:textColor="@color/purple_700"
                android:textSize="25sp"
                android:theme="@style/button2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/dua"
                app:layout_constraintTop_toTopOf="@+id/dua"
                tools:ignore="MissingConstraints" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>

